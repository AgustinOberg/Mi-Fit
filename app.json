{
  "expo": {
    "name": "mifit",
    "slug": "mifit",
    "version": "0.0.0",

    "package": "com.mifit.app",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.mifit.app"
    },
    "android": {
      "package": "com.mifit.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/segoe-ui-regular.ttf",
            "./assets/fonts/segoe-ui-bold.ttf",
            "./assets/fonts/segoe-ui-light.ttf",
            "./assets/fonts/segoe-ui-semilight.ttf",
            "./assets/fonts/segoe-ui-semibold.ttf"
          ]
        }
      ],
      "expo-secure-store",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.881710873579-58om08e97i6fqodlr4o1u0884bauc6l9"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
